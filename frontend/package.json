{
	"name": "frontend",
	"version": "1.0.0",
	"description": "",
	"scripts": {
		"prebuild": "rm -rf ./dist",
		"prestart": "rm -rf ./.cache",
		"start": "parcel serve src/index.html -p 9000 --cache-dir .cache",
		"build": "parcel build src/index.html --dist-dir dist",
		"lint": "eslint --ignore-path .eslintignore --ext .ts .",
		"lint:fix": "eslint --ignore-path .eslintignore --ext .ts . --fix",
		"format": "prettier --ignore-path .eslintignore --write \"**/*.+(ts)\"",
		"clean": "rm -rf ./dist ./.cache"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"eslint": "^8.7.0",
		"eslint-config-prettier": "^8.3.0",
		"parcel": "^2.2.1",
		"prettier": "^2.5.1",
		"typescript": "^4.5.4",
		"typescript-plugin-css-modules": "^3.4.0"
	}
}
